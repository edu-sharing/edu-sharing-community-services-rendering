{
  "name": "${composer.vendor}/community-services-rendering",
  "version": "${composer.version}",
  "type": "project",
  "description": "The rendering service brings edu-sharing objects to devices. Therefore it will convert objects, render them together with the respective ../../../.legacy/license and check for viewing permissions",
  "minimum-stability": "stable",
  "homepage": "http://www.edu-sharing.com/",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "metaVentis GmbH",
      "homepage": "http://www.metaventis.com/"
    }
  ],
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=8.1",
    "curl/curl": "~2.0",
    "guzzlehttp/guzzle": "~7.4.1",
    "apache/log4php": "~2.3.0",
    "dompdf/dompdf": "^1.0",
    "phpoffice/phppresentation": "^0.9.0",
    "phpoffice/phpspreadsheet": "^1.18",
    "phpoffice/phpword": "^0.18.2",
    "mathjax/mathjax": "^2.7.5"
  },
  "require-dev": {
    "vimeo/psalm": "~4.27.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.org"
    },
    {
      "type": "composer",
      "url": "${composer.repo.url}"
    }
  ]
}
